  <tabs>
    <Tab text="きほん">
      <content>
        <VBox fx:id="blockMenu" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
      </content>
    </Tab>
    <Tab text="おうよう">
      <content>
        <VBox fx:id="blockMenuAdv" prefHeight="200.0" prefWidth="100.0">
